                <!-- /. ROW  -->

                <div class="row">
                   
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                정치인 평점 관리
                            </div> 

                            <div class="panel-body">
                                <div class="table-responsive" style="
                                width: 100%;
                                height: 400px;
                                overflow-x: scroll;
                                overflow-y: scroll;
                                border-top: 1px solid #d0d0d0;">
                                    <table class="table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr>
                                                <th style="width:4%;">정치인 번호</th>
                                                <th style="width:4%;">정치인 이름</th>
                                                <th style="width:5%;">정당</th>
                                                <th style="width:5%;">선거구</th>
                                                <th style="width:4%;">평점받은횟수</th>
                                                <th style="width:5%;">평균 평점</th>
        
                                            </tr>
                                        </thead>
                                        <tbody>
 
                                            <% for(var i=0; i<gradeList.length ; i++) { %>
                                            <tr>
                                                <td><%= gradeList[i].politician_no %></td>
                                                <td><%= gradeList[i].politician_name %></td>
                                                <td><%= gradeList[i].jdName %></td>
                                                <td><%= gradeList[i].sggName %></td>
                                                <td><%= gradeList[i].count %></td>
                                                <td><%= gradeList[i].avg %></td>

                                            </tr>
                                            <% } %>



                                            


                                        </tbody>
                                    </table>
